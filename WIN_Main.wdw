    name : BTN_DUPLICATE
    identifier : 0x20eed4161852fbe8
    internal_properties : 
    properties :
     x : 11
     y : 283
     width : 213
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAADcq5tmydy60+MHe1SRz6lwKBXzsZbwEeu1DjQZvDc8dt8Fx3H7TJmkdzDJUvTZNW0iewek8536BRGschKS+/GlcWv38a+rbGjuNCWuURCzSsnRdLO4mZaihMXNaJuCRt8wABbG09Pf9cDGVxjP6A==
        code : |1-
         
         // Variables
         nID                is int
         sStatus             is string
         sBase          is string
         sDatabaseList is string
         
         // Checking the information entered
         IF EDT_Directory ~= “” THEN
         	Error(<§$0007§>)
         	RETURN
         END
         
         IF EDT_NewName ~= “” THEN
         	Error(<§$0008§>)
         	RETURN
         END
         
         IF gbOpened = False THEN
         	Error(<§$0009§>)
         	RETURN
         END
         
         IF EDT_NewName = COMBO_Database..DisplayedValue THEN
         	Error(<§$000b§>)
         	RETURN
         END
         
         // Display the progress bar
         PROGBAR_Main..Visible = True
         
         // Save the database to duplicate
         nID = HBackup(gcntConnection,COMBO_Database..DisplayedValue+“/*”,EDT_Directory,True,<§$000c§>+ DateSys(),PROGBAR_Main)
         
         // Check whether the backup is completed or not, otherwise we wait
         IF nID <> 0 THEN
         	sStatus = ExtractString(HInfoBackup(gcntConnection), 6)
         	WHILE Val(sStatus) = 0
         		Multitask(-2)
         		sStatus = ExtractString(HInfoBackup(gcntConnection), 6)
         	END
         ELSE
         	Error(StringBuild(<§$0004§>,HErrorInfo()))
         END
         
         // Duplicate the database
         IF Val(sStatus) = 1 THEN
         	HRestoreBackup(gcntConnection,EDT_Directory+“/”+nID,COMBO_Database..DisplayedValue,EDT_NewName,PROGBAR_Main)
         	
         	IF ErrorOccurred THEN
         		Error(StringBuild(<§$000d§>,ErrorInfo()))
         	ELSE
         		Info(<§$0005§>)
         	END
         	
         	// Re-fill the list of databases
         	sDatabaseList = HListDatabase(gcntConnection,hLstNormal)
         	ListDeleteAll(COMBO_Database)
         	FOR EACH string sBase of sDatabaseList SEPAREE by TAB
         		ListAdd(COMBO_Database,sBase)
         	END
         	
         	IF COMBO_Database..Occurrence > 0 THEN	
         		ListSelectPlus(COMBO_Database,1)
         		EDT_NewName = COMBO_Database..DisplayedValue+“_Duplic”
         	END
         ELSE
         	Error(StringBuild(<§$0004§>,HErrorInfo()))
         END
         
         CASE Error:
         Error(ErrorInfo(errMessage))
         CASE EXCEPTION:
         Error(ExceptionInfo(errMessage))
