        internal_properties : CAAAAAgAAABlbKQ3oW/8XbPUodfAFDp1CqyenEet9HEdxsRuagWzSewh8QmwT+Uz6r3SN1mY9exnI7VLQOHmM6iNHVLDk5PkW0fPk9oJp9FnaJDRuyWco0FuG3mLGZ9K2xRdmzE9DJa/81ymqCm5sPiqeGNG930xDDOhZ0E84V1YzTcuCb/qaQ==
        code : |1-
         
         // Variable
         sDatabaseList is string
         sBase          is string
         sVersion      is string
         
         // Initialize the connection
         gcntConnection..Source = EDT_MachineName+“:”+EDT_Port
         gcntConnection..User = EDT_User
         gcntConnection..Password = EDT_Password
         gcntConnection..Provider = hAccessHFClientServer 
         gcntConnection..Access = hOReadWrite 
         gcntConnection..CursorOptions = hClientCursor 
         
         // Open the connection
         IF HOpenConnection(gcntConnection) THEN
         	// Check whether it is a server 14 (or later)
         	sVersion = ExtractString(HInfoServer(gcntConnection,hInfoVersion),2,TAB)
         	
         	IF Val(sVersion) <= 12 THEN
         		Error(<§$0003§>)
         		gbOpened = False
         	ELSE	
         		Info(<§$0002§>)
         		gbOpened = True
         		
         		// Fill the list of databases
         		sDatabaseList = HListDatabase(gcntConnection,hLstNormal)
         		
         		ListDeleteAll(COMBO_Database)
         		FOR EACH string sBase of sDatabaseList SEPAREE by TAB
         			ListAdd(COMBO_Database,sBase)
         		END
         		
         		IF COMBO_Database..Occurrence > 0 THEN	
         			ListSelectPlus(COMBO_Database,1)
         			EDT_NewName = COMBO_Database..DisplayedValue+“_Duplic”
         		END
         	END
         	
         	GR_CONNECT..State = Active
         ELSE
         	Error(StringBuild(<§$0001§>,HErrorInfo()),<§$0000§>)
         	gbOpened = False
         	GR_CONNECT..State = Grayed
         END
